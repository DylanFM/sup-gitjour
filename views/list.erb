[
<% @jours.each do |(name, services)| %>
  {
    'name':'<%=name.to_s%>',
    'repos':[
    <% services.each do |service| %>
      {
        'name':'<%=service.name%>',
        'url':"git://<%= service.host.gsub(/\.$/,"") %><%= service.port == 9418 ? "" : ":#{service.port}" %>/<%= service.path %>"
      },
    <% end %>
    ]
  },
<% end %>
]
